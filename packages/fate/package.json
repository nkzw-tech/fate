{
  "name": "@nkzw/fate",
  "version": "0.1.1",
  "description": "fate is a modern data client for React.",
  "homepage": "https://github.com/nkzw-tech/fate",
  "license": "MIT",
  "author": {
    "name": "Christoph Nakazawa",
    "email": "christoph.pojer@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nkzw-tech/fate"
  },
  "bin": {
    "fate": "./lib/cli.mjs"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "main": "./lib/index.mjs",
  "types": "./lib/index.d.mts",
  "exports": {
    ".": {
      "types": "./lib/index.d.mts",
      "@nkzw/source": "./src/index.ts",
      "default": "./lib/index.mjs"
    },
    "./cli": {
      "types": "./lib/cli.d.mts",
      "@nkzw/source": "./src/cli.ts",
      "default": "./lib/cli.mjs"
    },
    "./server": {
      "types": "./lib/server.d.mts",
      "@nkzw/source": "./src/server.ts",
      "default": "./lib/server.mjs"
    }
  },
  "scripts": {
    "build": "tsdown -d lib --target=node24 src/index.ts src/server.ts src/cli.ts"
  },
  "dependencies": {
    "superjson": "^2.2.6",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@trpc/client": "^11.8.1",
    "@trpc/server": "^11.8.1"
  },
  "peerDependencies": {
    "@trpc/client": "^11.6.0"
  }
}
